
@{
    ViewBag.Title = "Register";
}

<h1>Register</h1>
<form method="post" enctype="multipart/form-data">
    <div class="form-group">CustomerName</div>
    <div><input type="text" name="customername" class="form-control" /></div>
    <div class="form-group">City</div>
    <div><input type="text" name="city" class="form-control" /></div>
    <div class=" form-group">Country</div>
    <div><input type="text" name="country" class="form-control" /></div>
  
    
    
 
    <div>@ViewBag.ErrorMessage</div>
    <div><button type="submit" class="btn-success">Submit Here</button></div>

</form>

